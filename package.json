		]
	},
	"dependencies": {
		"playwright-core": "^1.40.0"
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
