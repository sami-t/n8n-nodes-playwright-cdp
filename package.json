{
	"name": "@bednij.bohdan/n8n-nodes-playwright-cdp",
	"version": "1.0.0",
	"description": "n8n node to execute Playwright code via CDP connection to running browsers",
	"license": "MIT",
	"homepage": "https://github.com/oneassasin/n8n-nodes-playwright-cdp",
	"keywords": [
		"n8n-community-node-package",
		"playwright",
		"cdp",
		"browser-automation",
		"antidetect"
	],
	"author": {
		"name": "Bogdan"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/oneassasin/n8n-nodes-playwright-cdp.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
